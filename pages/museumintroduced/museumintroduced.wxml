<view class="bc">
    <view class="cell" wx:for="{{tabledata}}" wx:key="item.id">
        <view class="text">
        {{item.title}}
        </view>
        <rich-text nodes="{{item.htmlSnip}}"></rich-text>
        <view class="picture">
            <van-image
            width="10rem"
            height="10rem"
            fit="contain"
            src="{{item.imgurl}}"
            />
        </view>
        
    </view>
    
</view>